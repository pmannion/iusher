<h1>Editing complaint</h1>
<div id="edit-complaint">



  <%= form_for(@complaint) do |f| %>
    <%= f.label :status %><br />
    <%= f.select :status, Complaint::STATUS_TYPES, prompt: "select a status" %>
    <%= f.submit 'update complaint', class: 'btn btn-info'%>
  <% end %>


<%= link_to 'Show', @complaint, class: 'btn btn-success' %>
<%= link_to 'Back', complaints_path, class: 'btn btn-success' %>
</div>
